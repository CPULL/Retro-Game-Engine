 Name: Pong (actually squash)
Start {
	clr(c003)
	x = 256
	y = 160
	f=0
}

Update {
	clr(c003)
	Write(x+","+y + " / " + f, 4, 4, c300)
	box(100, 100, 160, 140, c330, c030)
	
	if (keyL() & x>160) {
		x-=8
	}
	if (keyR() & x < 320) {
		x+=8
	}
	if (keyD() & y>100) {
		y-=8
	}
	if (keyU() & y<256) {
		y+=8
	}
	if (keyA()) {
		f=!f
	}
	

	if (keyF(1)) {
		screen(x,y,f)
	}
}
